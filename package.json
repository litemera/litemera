{
  "name": "litemera",
  "version": "0.1.0",
  "private": true,
  "description": "Take a photo with Litemera, it will be deleted automatically after 24 hours.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add . && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages && git push"
